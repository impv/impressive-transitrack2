{
	"$schema": "https://unpkg.com/knip/schema.json",
	"entry": ["src/pages/**/*.{ts,tsx}"],
	"ignore": ["next.config.js", ".next/**", "public/**"],
	"project": ["src/**/*.ts", "src/**/*.tsx"],
	"rules": {
		"dependencies": "off",
		"devDependencies": "off",
		"unlisted": "off",
		"binaries": "off",
		"unresolved": "off"
	},
	"ignoreDependencies": ["tailwindcss", "postcss"]
}
